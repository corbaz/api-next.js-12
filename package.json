{
  "name": "nextjs12-app",
  "private": true,
  "scripts": {
    "dev": "npm run format && next dev",
    "git": "npm run format && git add -A && git commit -m 'update' && git push --set-upstream origin main",
    "format": "cls && npx prettier --write .",
    "ncu:check": "ncu",
    "ncu:upgrade": "ncu -u",
    "banch-main": "git checkout main",
    "banch-boilerplate": "git checkout boilerplate",
    "push-boilerplate": "git add -A && git commit -m 'update' && git push origin boilerplate",
    "git-init": "git init && git remote add origin https://github.com/corbaz/api-next.js-12.git && git branch -M main",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "daisyui": "1.16.2",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "eslint": "8.4.0",
    "eslint-config-next": "12.0.7",
    "postcss": "^8.4.4",
    "prettier": "2.5.1",
    "tailwindcss": "^2.2.19"
  }
}
